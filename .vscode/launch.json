    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/.",
            "envFile": "${workspaceFolder}/.vscode/.env",
            "env": {
                "GUILD_CHANNEL_DELETE_OFFSET": "10s",
                "GUILD_CHANNEL_ACCESS_OFFSET": "2h",
                "REQUIREMENTS_OFFSET": "30m",
                "ASYNC_LOOP_INTERVAL": "1s",
                "BACKOFF_MIN_DURATION": "1s",
                "REMINDER_INTERVALS": "1m,5m,15m,30m,1h,2h",
                //"DSN": "${workspaceFolder}/league.db",
                //"DSN": "./league.db",
            },

        }
    ]
}